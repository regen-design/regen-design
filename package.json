{
  "name": "regen-design",
  "version": "0.0.0",
  "description": "A unique design style and React ui component library",
  "main": "index.js",
  "type": "module",
  "homepage": "https://regen-design.github.io/regen-design/",
  "scripts": {
    "prepare": "husky install",
    "lint": " eslint \"packages/**/*.+(cjs|js|ts|jsx|tsx)\"",
    "format": "npx prettier --write \"packages/**/*.+(cjs|js|ts|jsx|tsx)\"",
    "docs:dev": "npx pnpm -C docs start",
    "docs:build": "npx pnpm -C docs build",
    "test": "vitest",
    "icons:generate": "npx pnpm -C packages/icons generate",
    "generate:components": "npx pnpm -C scripts generate:components"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/regen-design/regen-design"
  },
  "keywords": [
    "react",
    "react-component",
    "react-components",
    "react-library",
    "react-components-library",
    "react-ui",
    "react-ui-library",
    "react-ui-components"
  ],
  "author": "@regen-design",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "devDependencies": {
    "@babel/generator": "^7.23.6",
    "@babel/parser": "^7.23.6",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@emotion/is-prop-valid": "^1.2.1",
    "@regen-design/constant": "workspace:^",
    "@testing-library/jest-dom": "^6.2.1",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/react-test-renderer": "^18.0.7",
    "@types/react-transition-group": "^4.4.10",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "classnames": "^2.5.1",
    "commander": "^11.1.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "happy-dom": "^13.2.1",
    "highlight.js": "^11.9.0",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "polished": "^4.2.2",
    "prettier": "^3.2.1",
    "raw-loader": "^4.0.2",
    "react-markdown": "^9.0.1",
    "react-transition-group": "^4.4.5",
    "readline": "^1.3.0",
    "styled-components": "^6.1.8",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "@regen-design/components": "workspace:^",
    "@regen-design/hooks": "workspace:^",
    "@regen-design/icons": "workspace:^",
    "@regen-design/theme": "workspace:^",
    "@regen-design/types": "workspace:^",
    "@regen-design/utils": "workspace:^",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}