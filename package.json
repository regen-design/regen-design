{
  "name": "regen-design",
  "version": "0.0.1-beta.4",
  "description": "A unique design style and React ui component library",
  "main": "dist/lib/index.js",
  "types": "./index.d.ts",
  "module": "dist/es/index.js",
  "unpkg": "dist/lib/index.js",
  "jsdelivr": "dist/lib/index.js",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "homepage": "https://regen-design.github.io/regen-design/",
  "files": [
    "dist",
    "/LICENSE",
    "README.md"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": " eslint \"packages/**/*.+(cjs|js|ts|jsx|tsx)\"",
    "format": "npx prettier --write \"packages/**/*.+(cjs|js|ts|jsx|tsx)\"",
    "build": "npx pnpm run rollup && npx  pnpm run afterBuild",
    "docs:dev": "npx pnpm -C docs start",
    "docs:build": "npx pnpm -C docs build",
    "test": "vitest",
    "icons:generate": "npx pnpm -C packages/icons generate",
    "gen:components": "npx pnpm -C scripts gen:components",
    "afterBuild": "npx pnpm -C scripts afterBuild",
    "rollup": "npx rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/regen-design/regen-design"
  },
  "keywords": [
    "react",
    "react-component",
    "react-components",
    "react-library",
    "react-components-library",
    "react-ui",
    "react-ui-library",
    "react-ui-components"
  ],
  "author": "@regen-design",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cjs"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/generator": "^7.23.6",
    "@babel/parser": "^7.23.6",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/runtime": "^7.23.8",
    "@babel/runtime-corejs3": "^7.23.8",
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^18.5.0",
    "@commitlint/config-conventional": "^18.5.0",
    "@emotion/is-prop-valid": "^1.2.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-run": "^3.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^14.1.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.6",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-test-renderer": "^18.0.7",
    "@types/react-transition-group": "^4.4.10",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "classnames": "^2.5.1",
    "commander": "^11.1.0",
    "comment-parser": "^1.4.1",
    "cz-customizable": "^7.0.0",
    "esbuild": "^0.19.12",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "fs-extra": "^11.2.0",
    "grapheme-splitter": "^1.0.4",
    "happy-dom": "^13.3.1",
    "highlight.js": "^11.9.0",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "less": "^4.2.0",
    "polished": "^4.2.2",
    "prettier": "^3.2.4",
    "raw-loader": "^4.0.2",
    "react-markdown": "^9.0.1",
    "react-transition-group": "^4.4.5",
    "readline": "^1.3.0",
    "rollup": "^4.9.6",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "styled-components": "^6.1.8",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.1"
  },
  "dependencies": {
    "@regen-design/components": "workspace:^",
    "@regen-design/constant": "workspace:^",
    "@regen-design/hooks": "workspace:^",
    "@regen-design/icons": "workspace:^",
    "@regen-design/theme": "workspace:^",
    "@regen-design/types": "workspace:^",
    "@regen-design/utils": "workspace:^",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
